    "@types/node-fetch": "^2.1.1",
    "file-loader": "^6.0.0",
    "json-loader": "^0.5.7",
    "nodemon": "^1.12.1",
    "prettier": "^2.0.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.7.4",
    "uglifyjs-webpack-plugin": "^1.2.5",
